<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--
    Base application theme, supporting dark and light themes.
    The actual Compose theme is applied in AlibreTheme.kt
    -->
    <style name="Theme.Alibre" parent="android:Theme.Material.Light.NoActionBar" />

    <style name="Theme.Alibre.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>
</resources>
